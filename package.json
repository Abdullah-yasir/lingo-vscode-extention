{
	"name": "lingo",
	"displayName": "Lingo",
	"description": "Syntax highlighting for lingo",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.78.0"
	},
	"icon": "https://github.com/Abdullah-yasir/lingo-vscode-extention/blob/main/assets/logo.png",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "lingo",
				"aliases": [
					"lingo",
					"lingo"
				],
				"extensions": [
					".lo"
				],
				"icon": {
					"light": "./assets/icon.svg",
					"dark": "./assets/icon.svg"
				},
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "lingo",
				"scopeName": "source.lingo",
				"path": "./syntaxes/lingo.tmLanguage.json"
			}
		]
	}
}
