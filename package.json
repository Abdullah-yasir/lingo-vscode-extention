{
	"name": "cowlang",
	"displayName": "Cowlang Language Support",
	"description": "Syntax highlighting for cowlang",
	"author": {
		"name": "skmaky"
	},
	"version": "1.0.0",
	"repository": {
		"url": "https://github.com/Abdullah-yasir/lingo-vscode-extention"
	},
	"engines": {
		"vscode": "^1.78.0"
	},
	"icon": "https://raw.githubusercontent.com/Abdullah-yasir/lingo-vscode-extention/main/assets/logo.png",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "cowlang",
				"aliases": [
					"cowlang",
					"cowlang"
				],
				"extensions": [
					".cow"
				],
				"icon": {
					"light": "./assets/icon.svg",
					"dark": "./assets/icon.svg"
				},
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "cowlang",
				"scopeName": "source.cowlang",
				"path": "./syntaxes/lingo.tmLanguage.json"
			}
		]
	}
}
